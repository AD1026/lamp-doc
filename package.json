{
  "name": "lamp-cloud-doc",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "bootstrap": "pnpm i",
    "dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit",
    "docs:lint": "prettier --check --write docs/",
    "docs:clean-dev": "vuepress dev docs --no-cache",
    "packages:check-update": "pnpm dlx npm-check-updates -u --deep --timeout 600000",
    "packages:update": "pnpm dlx vp-update"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.61",
    "@vuepress/plugin-docsearch": "2.0.0-beta.61",
    "@vuepress/utils": "2.0.0-beta.61",
    "vue": "^3.2.47",
    "vuepress": "2.0.0-beta.61",
    "vuepress-theme-hope": "2.0.0-beta.201"
  },
  "devDependencies": {
    "prettier": "2.8.4",
    "increase-memory-limit": "^1.0.7",
    "vuepress-plugin-search-pro": "2.0.0-beta.202"
  },
  "packageManager": "pnpm@7.29.1",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
